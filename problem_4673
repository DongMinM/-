num = list(range(1,10001))
def d(n):
    sum = n
    str_n = str(n)
    for i in range(0,len(str_n)):
        sum += int(str_n[i])
    return sum

for n in range(1,10000):
    while n <= 10000:
        n = d(n)
        if n <= 10000:
        
            num[n-1] = -1
            
for i in num:
    if i != -1:
        print(i)
